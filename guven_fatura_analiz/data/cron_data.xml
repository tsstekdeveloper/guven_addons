<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="ir_cron_fetch_invoice_details" model="ir.cron">
            <field name="name">E-Fatura Detay Çekme ve Parse</field>
            <field name="model_id" ref="model_guven_fatura"/>
            <field name="state">code</field>
            <field name="code">model._cron_fetch_invoice_details()</field>
            <field name="interval_number">2</field>
            <field name="interval_type">minutes</field>
            <field name="active">False</field>
        </record>

        <record id="ir_cron_fetch_earsiv_details" model="ir.cron">
            <field name="name">E-Arşiv Detay Çekme ve Parse</field>
            <field name="model_id" ref="model_guven_fatura"/>
            <field name="state">code</field>
            <field name="code">model._cron_fetch_earsiv_details()</field>
            <field name="interval_number">2</field>
            <field name="interval_type">minutes</field>
            <field name="active">False</field>
        </record>

        <record id="ir_cron_sync_headers" model="ir.cron">
            <field name="name">E-Fatura/E-Arşiv Header Senkronizasyonu</field>
            <field name="model_id" ref="model_guven_fatura"/>
            <field name="state">code</field>
            <field name="code">model._cron_sync_headers()</field>
            <field name="interval_number">10</field>
            <field name="interval_type">minutes</field>
            <field name="active">True</field>
            <field name="priority">5</field>
        </record>

        <record id="ir_cron_logo_sync" model="ir.cron">
            <field name="name">Logo MSSQL Fatura Senkronizasyonu</field>
            <field name="model_id" ref="model_guven_logo_fatura"/>
            <field name="state">code</field>
            <field name="code">model._cron_sync_logo()</field>
            <field name="interval_number">15</field>
            <field name="interval_type">minutes</field>
            <field name="active">False</field>
            <field name="priority">10</field>
        </record>

    </data>
</odoo>
